{
  "name": "next-app-tutorial",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:worker": "npx @cloudflare/next-on-pages",
    "start": "next start",
    "lint": "next lint",
    "worker:dev": "cd workers && npx wrangler dev --port 8787",
    "pages:dev": "npx wrangler pages dev .vercel/output/static --compatibility-date=2025-07-17 --compatibility-flag=nodejs_compat",
    "pages:deploy": "npm run build && npm run build:worker && npx wrangler pages deploy .vercel/output/static --project-name=next-website-yukue",
    "build:pages": "next build",
    "deploy:pages": "npm run build:pages",
    "build:cloudflare": "npx @cloudflare/next-on-pages",
    "dev:cloudflare": "npx @cloudflare/next-on-pages --dev",
    "deploy:cloudflare": "npm run build:cloudflare && npx wrangler pages deploy .vercel/output/static --project-name=yukue-month"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.2",
    "axios": "^1.10.0",
    "canvas-confetti": "^1.9.3",
    "cheerio": "^1.1.0",
    "clsx": "^2.1.1",
    "daisyui": "^5.0.46",
    "framer-motion": "^12.23.3",
    "lodash.throttle": "^4.1.1",
    "lucide-react": "^0.525.0",
    "motion": "^12.23.1",
    "next": "15.3.4",
    "open-graph-scraper": "^6.10.0",
    "puppeteer": "^24.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.12",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "postcss": "^8.5.6",
    "typescript": "^5"
  }
}
